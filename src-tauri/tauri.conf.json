{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "clerk-nextjs-tauri-example",
  "version": "0.1.0",
  "identifier": "com.clerk-nextjs-tauri-example.dev",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "clerk-nextjs-tauri-example",
        "width": 600,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      },
      {
        "label": "command-palette",
        "url": "http://localhost:3000/command-palette",
        "decorations": false,
        "shadow": false,
        "transparent": true,
        "backgroundColor": "#2d82e308",
        "create": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
